{
    "name": "bracket-lens",
    "displayName": "Bracket Lens",
    "description": "Show bracket head on closing bracket.",
    "icon": "images/bracketlenshex.128.png",
    "version": "0.0.1",
    "license": "SEE LICENSE IN LICENSE_1_0.txt",
    "engines": {
        "vscode": "^1.50.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:bracket-lens.helloWorld"
    ],
    "main": "./out/source/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "bracket-lens.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "@wraith13/vscel": "^0.0.22"
    },
    "devDependencies": {
        "@types/vscode": "^1.50.0",
        "@types/node": "^12.11.7",
        "eslint": "^7.9.0",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    }
}
